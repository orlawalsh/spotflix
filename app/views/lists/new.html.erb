<div id="song_show">
      <div id="show_banner">
        <div class="wrapper_skinny">
          <h1>Ed Sheeran Dashboard</h1>
          
        </div>
      </div>

      <div id="links">
        <div class="wrapper_skinny">
          <ul>
            <li><%= link_to "Songs", root_path %> </li>
            <li><%= link_to "New Song", new_song_list_path(@song), class: "current" %></li>
            <li> <%= link_to "Account Settings", edit_song_registration_path %></li>
            <li><%= link_to "View Songs", song_path(current_song) %></li>
          </ul>
        </div>
      </div>
      <div id="form">
    <div class="wrapper_skinny">
		<%= form_for ([@song, @list]) do |f| %>
			<div class="field">
				<%= f.label :title %><br>
				<%= f.text_field :title %>
			</div>

			<div class="field">
				<%= f.label :description %><br>
				<%= f.text_area :description %>
			</div>

	<br>
	<br>

	<%= f.submit class: "button button_highlight button_block" %>
		<% end %>
	</div>
</div>
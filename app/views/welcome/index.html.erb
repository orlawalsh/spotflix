<div id="background">
			<header class="cf">
				<div class="wrapper_wide">
					<a href="#" id="logo">Spotflix</a>
					<nav>
						<ul>
							<% unless song_signed_in? %>
							<li><%= link_to "Explore", songs_path %></li>
							<li><%= link_to "Sign In", new_song_session_path%></li>
							<li><%= link_to "Create Playlist", new_song_registration_path%></li>
							<% else %>
							<li><a href="#">Dashboard</a></li>
							<li><%= link_to "Explore", songs_path %></li>
							<li><%= link_to "Sign Out", destroy_song_session_path, method: :delete %> </li>
							<% end %>
						</ul>

					</nav>
					</div>
			</header>
			<div class="wrapper_skinny">
				<h1>Discover new music<br>or add your own!</h1>
				<%= link_to "Create Playlist", new_song_registration_path, class: "button button_highlight"%>

				<%= link_to "Explore", songs_path, class: "button button_white" %>
			</div>

		</div>